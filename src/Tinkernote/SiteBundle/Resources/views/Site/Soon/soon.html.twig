<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8" />
    <title>{% block title %}Tinkernote | Revient {% endblock %}</title>
    {% block stylesheets %}
        {% stylesheets  filter='cssrewrite'
        'bundles/site/css/bootstrap.css'
        'bundles/site/css/style.css'
        %}
        <link rel="stylesheet" href="{{ asset_url }}"/>
        {% endstylesheets %}
    {% endblock %}
    <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
</head>
<body>

{% include ('SiteBundle:Site/Template:header.html.twig') %}

{% block body %}
    {% include('SiteBundle:Site/Soon:soon_content.html.twig') %}
{% endblock %}


{% block javascripts %}
    {% javascripts 'bundles/site/js/jquery-2.1.1.min.js'
    'bundles/site/js/bootstrap.min.js'
    'bundles/site/js/myfunction.js' %}
    <script type="text/javascript" src="{{ asset_url }}"></script>
    {% endjavascripts %}
{% endblock %}
</body>
</html>

