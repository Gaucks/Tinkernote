<!DOCTYPE html><html>    <head>        <meta charset="UTF-8" />        <title>{% block title %}Tinkernote{% endblock %}</title>        {% block stylesheets %}            {% stylesheets  filter='cssrewrite'                                                'bundles/site/css/bootstrap.css'                                                'bundles/site/css/style.css'                                                'bundles/site/css/annonce.css'                                                'bundles/site/css/correctif.css'                                                'bundles/site/css/font-awesome.min.css'            %}            <link rel="stylesheet" href="{{ asset_url }}"/>            {% endstylesheets %}        {% endblock %}        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />    </head>    <body>        {% include ('SiteBundle:Site/Template:header.html.twig') %}        {% block body %}{% endblock %}        {% include 'SiteBundle:Site:Accueil/Template/footer_content.html.twig' %}        {% block javascripts %}            {% javascripts 'bundles/site/js/jquery-2.1.1.min.js'                           'bundles/site/js/bootstrap.min.js'                           'bundles/site/js/myfunction.js' %}            <script type="text/javascript" src="{{ asset_url }}"></script>            {% endjavascripts %}        {% endblock %}    </body></html>